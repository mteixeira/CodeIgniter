<!-- Default Table Widget -->
<div class="table-responsive">
  <table class="table table-hover">
    <thead>
        <tr>
          {% block table_head %}
            {% for entry in entries  %}<!--for 1  {{entry.id}}-->
              {% if loop.first %}
                {% for key,value in entry  %}<!--for 2-->
                    {% include [
    "twig/"~app.controller~"/table_csolumn.html.twig", 
    'twig/default/table_column.html.twig'
] with {type:"Header"} %}
                {% endfor %}    
              {% endif %}
            {% endfor %}
          {% endblock %}
        </tr>
    </thead>
    <tbody>
      {% for rows in entries %}
          <tr>
            {% block table_row %}
              {% for key,value in rows %}
                    {% include [
    "twig/"~app.controller~"/table_coslumn.html.twig", 
    'twig/default/table_column.html.twig'
]  with {type:"Body"} %}
              {% endfor %}  
              
            {% endblock %}
          <tr>
      {% endfor %}  
      
      {# 
       <tr>
            <th>1</th>
            <td>Kolor Tea Shirt For Man</td>
